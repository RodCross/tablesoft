	<queryString>
		<![CDATA[SELECT
    'No asociados a activo fijo' as Nombre,
    A.total_naf as Cantidad
FROM (select count(*) as total_naf

      from ticket 
       WHERE fecha_envio >  $P{FechaInicio} 
      and fecha_envio <   $P{FechaFin} and activo_fijo_id is null) A
UNION ALL
