	<parameter name="FechaInicio" class="java.sql.Timestamp"/>
	<parameter name="FechaFin" class="java.sql.Timestamp"/>
	<parameter name="Equipo_id" class="java.lang.Integer"/>
	<parameter name="Total" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
    'No recategorizados' as Nombre,
    A.total_categoria as Cantidad
FROM (SELECT count(T.ticket_id) - E.total_escalados AS total_categoria
      FROM (select count(distinct C.ticket_id) as total_escalados
             from cambio_estado_ticket C
             where C.ticket_id in (select T.ticket_id 
                                   from ticket T
                                   INNER JOIN agente A ON T.agente_id = A.agente_id
                                   WHERE A.equipo_id =   $P{Equipo_id}  
                                   and T.fecha_envio > $P{FechaInicio} 
                                   and T.fecha_envio <  $P{FechaFin} )
             and C.estado_id_to = 5) E,
             ticket T
      INNER JOIN agente A ON T.agente_id = A.agente_id
      WHERE A.equipo_id =   $P{Equipo_id} 
      AND T.fecha_envio > $P{FechaInicio}
      AND T.fecha_envio < $P{FechaFin}) A
UNION ALL
SELECT 
    'Recategorizados' as Nombre,
    B.total_escalados as Cantidad
FROM (select count(distinct C.ticket_id) as total_escalados
       from cambio_estado_ticket C
       where C.ticket_id in (select T.ticket_id 
                             from ticket T
                             INNER JOIN agente A ON T.agente_id = A.agente_id
                             WHERE A.equipo_id =   $P{Equipo_id}  
                             and T.fecha_envio > $P{FechaInicio} 
                             and T.fecha_envio <  $P{FechaFin} )
       and C.estado_id_to = 5) B]]>
	</queryString>
