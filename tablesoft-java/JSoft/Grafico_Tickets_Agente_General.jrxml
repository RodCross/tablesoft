	<queryString>
		<![CDATA[select CONCAT(P.nombre, ' ', P.apellido_paterno, ' ',P.apellido_materno , " (" , P.codigo, ')') as nombre, count(T.ticket_id) as cantidad
from ticket T
inner join agente A
on T.agente_id = A.agente_id
inner join persona P 
on P.codigo = A.codigo
inner join equipo E 
on E.equipo_id = A.equipo_id
where T.fecha_envio >  $P{FechaInicio} 
and T.fecha_envio < $P{FechaFin} 
group by A.agente_id;]]>
	</queryString>
