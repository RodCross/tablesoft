	<parameter name="Ticket_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select T.tarea_id, 
CONCAT(P.nombre, " " ,P.apellido_paterno, " ",  P.apellido_materno) as nombrePersona,
T.fecha_creacion,
T.fecha_completado, 
T.descripcion,
T.completado
from tarea T
inner join agente A
on T.agente_id = A.agente_id 
inner join persona P
on P.codigo = A.codigo  
where T.ticket_id =  $P{Ticket_id} ;]]>
