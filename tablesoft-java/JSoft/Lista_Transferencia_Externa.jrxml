	<parameter name="Ticket_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select TT.transferencia_id, 
CONCAT(P.nombre, " " ,P.apellido_paterno, " ",  P.apellido_materno) as nombrePersona,
TT.fecha_transferencia, 
TT.comentario,
C.nombre as nombreCategoria
from transferencia_ticket TT
inner join transferencia_interna TI
on TI.transferencia_id = TT.transferencia_id
inner join agente A
on TT.agente_id = A.agente_id 
inner join persona P
on P.codigo = A.codigo 
inner join categoria C
on C.categoria_id = TI.categoria_id_to
where TT.ticket_id = $P{Ticket_id}  ;]]>
	</queryString>
