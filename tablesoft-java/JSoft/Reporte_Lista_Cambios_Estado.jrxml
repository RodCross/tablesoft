	<parameter name="TicketId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT CET.cambio_estado_id, CONCAT(P.nombre, " " ,P.apellido_paterno, " ",  P.apellido_materno) as nombreAgente, CET.fecha_cambio_estado, ET.nombre, CET.comentario 
FROM cambio_estado_ticket CET 
inner join estado_ticket ET on CET.estado_id_to = ET.estado_id 
inner join agente A on CET.agente_id = A.agente_id 
inner join persona P on P.codigo = A.codigo 
where CET.ticket_id =    $P{TicketId}  order by cambio_estado_id;]]>
