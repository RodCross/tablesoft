	<queryString language="SQL">
		<![CDATA[SELECT A.total_categoria,
    B.total_retrasados
FROM (SELECT count(*) AS total_categoria
      FROM ticket 
      WHERE agente_id = $P{Agente_id} 
      AND fecha_envio > $P{FechaInicio} 
      AND fecha_envio <  $P{FechaFin}) A,
      (select count(*) as total_retrasados
       from ticket
       where retrasado = 1  
                            AND fecha_envio > $P{FechaInicio} 
      AND fecha_envio <  $P{FechaFin} and  agente_id = $P{Agente_id} ) B;]]>
	</queryString>
